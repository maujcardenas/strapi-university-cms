{
  "kind": "collectionType",
  "collectionName": "course_completions",
  "info": {
    "singularName": "course-completion",
    "pluralName": "course-completions",
    "displayName": "Course Completion",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "segment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::segment.segment"
    },
    "student": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::student.student"
    },
    "subject": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::subject.subject"
    },
    "Grade": {
      "type": "enumeration",
      "enum": [
        "A",
        "B",
        "C",
        "D",
        "F",
        "Not yet graded"
      ],
      "default": "Not yet graded"
    },
    "currentCourseHours": {
      "type": "decimal",
      "default": 0
    },
    "isCompleted": {
      "type": "boolean",
      "default": false
    }
  }
}
